
#include <stdio.h>
int main(){
    int a[]={5,20,22,11,6};
    int i=0,max=0,min=0;
    int n=sizeof(a)/sizeof(a[0]);
    for(i=0;i<n;i++){
        if(a[i]>max)
        max=a[i];
    else
    {
    if(a[i]<max)
    min<max;
    min=a[i];
    }
    }
    printf("\nthe max value is %d",max);
    printf("\nthe min value is %d",min);

    return 0;
}
